{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
				"jest/globals": true
    },
    "rules": {
			"require-jsdoc": "off",
			"linebreak-style": "off",
			"no-tabs": "off",
			"no-unused-vars": "off",
			"no-invalid-this": "off"
    },
		"plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}